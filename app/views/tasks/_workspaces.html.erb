<div class="data_list" style="height: 400px; border: none;">
    <div class="left">
        <div id="workspace_left" class="workspaces">
          <%= render :partial => "left_workspace"%>
        </div>
    </div>
    <% if @user.types == User::TYPES[:PPT] || @user.types == User::TYPES[:FLASH]%>
    <div class="right">
        <div id="workspace_right" class="workspaces">
          <%= render :partial => "right_workspace"%>
        </div>
    </div>
    <% end %>
</div>