<table style="text-align: center">
  <tr>
    <td>任务批次</td>
    <td>上传日期</td>
    <td>状态</td>
    <td>下载任务</td>
    <td>确认终检</td>
  </tr>
  <%@task_tags.each do |task_tag| %>
    <tr>
      <td><%= task_tag.name%></td>
      <td><%= task_tag.time%></td>
      <td><%= task_tag.scc%>|<%= task_tag.suu%></td>
      <td><input type="button" value="下载任务"/></td>
      <td><input type="button" value="确认终检"/></td>
    </tr>
  <% end %>
</table>